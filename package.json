  "contributors": [
    "Takuya Hayashi <qrbys@outlook.com>"
  ],
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/chrome": "^0.0.77",
    "bestzip": "^2.1.2",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "release-it": "^9.8.0",
    "shx": "^0.3.2",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.2",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.2.1"
  },
  "scripts": {
    "build": "yarn build:prod",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "watch": "yarn build:dev --watch",
    "zip": "shx cp -R dist ${NAME} && bestzip ${NAME}.zip ${NAME}/* && shx rm -rf ${NAME}",
    "release": "release-it",
    "ci": "npm-run-all --parallel ci:*",
    "ci:type": "tsc --project . --noEmit",
    "ci:lint:": "tslint --project . --format stylish"
  }
}
